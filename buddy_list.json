{
  "version": "0.0.6",
  "appTitle": "buddy_list",
  "logo": "https://wxccdemo.s3.us-west-1.amazonaws.com/allVerticals/webex.png",
  "dragDropEnabled": false,
  "notificationTimer": 8,
  "maximumNotificationCount": 3,
  "browserNotificationTimer": 8,
  "wxmConfigured": false,
  "desktopChatApp": {
    "webexConfigured": true
  },
  "area": {
    "headless": {
      "id": "dw-headless",
      "widgets": { "comp1": { "comp": "div" } },
      "comp2": {
                "comp": "headless-crm-widget",
                "script": "https://webextoolsde.github.io/Call-History/src/build/buddy-list.js",
                "attributes": {
                  "token": "$STORE.auth.accessToken",
                  "avatar": "https://randomuser.me/api/portraits/med/lego/1.jpg",
                  "orgId": "$STORE.agent.orgId",
                  "name": "Im a property from the JSON layout",
                  "teamId":"$STORE.agent.teamId",
                  "darkmode": "$STORE.app.darkMode",
                  "myOutdialEPID":"2df8d2e9-aac0-4b58-8bff-acb975c1d74c",
                  "title":"Buddy List for my Company",
                  "titletext":"Text for meine BuddyList",
                  "region":"us1"
                }
            },
      "layout": { "areas": [["comp1","comp2"]], "size": { "cols": [1,1], "rows": [1] } }
    },
    "panel": {
      "comp": "md-tabs",
      "attributes": { "class": "widget-tabs" },
      "children": [
        {
          "comp": "md-tab",
          "attributes": { "slot": "tab", "class": "widget-pane-tab" },
          "children": [
            { "comp": "slot", "attributes": { "name": "IVR_TRANSCRIPT_TAB" } }
          ],
          "visibility": "IVR_TRANSCRIPT"
        },
        {
          "comp": "md-tab-panel",
          "attributes": { "slot": "panel", "class": "widget-pane" },
          "children": [
            { "comp": "slot", "attributes": { "name": "IVR_TRANSCRIPT" } }
          ],
          "visibility": "IVR_TRANSCRIPT"
        },
        {
          "comp": "md-tab",
          "attributes": { "slot": "tab" },
          "children": [
            { "comp": "slot", "attributes": { "name": "WXM_JOURNEY_TAB" } }
          ],
          "visibility": "WXM_JOURNEY"
        },
        {
          "comp": "md-tab-panel",
          "attributes": { "slot": "panel", "class": "widget-pane" },
          "children": [
            {
              "comp": "agentx-wc-cloudcherry-widget",
              "properties": {
                "userModel": "$STORE.app.userModel",
                "spaceId": "",
                "metricsId": "",
                "teamId": "$STORE.agent.teamName",
                "ani": "$STORE.agentContact.taskSelected.ani",
                "isDarkMode": "$STORE.app.darkMode"
              },
              "wrapper": {
                "title": "Customer Experience Journey",
                "maximizeAreaName": "app-maximize-area"
              }
            }
          ],
          "visibility": "WXM_JOURNEY"
        },
        {
          "comp": "md-tab",
          "attributes": { "slot": "tab", "class": "widget-pane-tab" },
          "children": [
            { "comp": "slot", "attributes": { "name": "CONTACT_HISTORY_TAB" } }
          ]
        },
        {
          "comp": "md-tab-panel",
          "attributes": { "slot": "panel", "class": "widget-pane" },
          "children": [
            { "comp": "slot", "attributes": { "name": "CONTACT_HISTORY" } }
          ]
        },
        {
          "comp": "md-tab",
          "attributes": { "slot": "tab", "class": "widget-pane-tab" },
          "children": [
            { "comp": "slot", "attributes": { "name": "SCREEN_POP_TAB" } }
          ],
          "visibility": "SCREEN_POP"
        },
        {
          "comp": "md-tab-panel",
          "attributes": { "slot": "panel", "class": "widget-pane" },
          "children": [
            { "comp": "slot", "attributes": { "name": "SCREEN_POP" } }
          ],
          "visibility": "SCREEN_POP"
        }
      ]
    },
    "navigation": [
      {
          "nav": {
            "label": "Buddy List",
            "icon": "https://qaemailmedia.s3.amazonaws.com/348e2d9f-90b3-4dc7-b737-081bc8cbfcbe/7213251_3997020172948318.png",
            "iconType": "other",
            "navigateTo": "buddyList1",
            "align": "top"
          },
          "page": {
            "id": "buddyList1",
            "widgets": {
              "console": {
                "comp": "pay-widget",
                "script": "https://webextoolsde.github.io/Call-History/src/build/buddy-list.js",
                "attributes": {
                  "isDarkMode": "$STORE.app.darkMode",
                  "agentName": "$STORE.agent.agentName",
                  "agentEmailId": "$STORE.agent.agentEmailId",
                  "bearerToken": "$STORE.auth.accessToken",
                  "teamId":"$STORE.agent.teamId",
                  "teamName":"$STORE.agent.teamName",
                  "organizationId": "$STORE.agent.orgId"
                },
                "wrapper": {
                  "title": "Buddy List",
                  "maximizeAreaName": "app-maximize-area"
                }
              }
            },
            "layout": {
              "areas": [
                [
                  "console"
                ]
              ],
              "size": {
                "cols": [
                  1
                ],
                "rows": [
                  1
                ]
              }
            }
          }
        },
      {
        "nav": {
          "label": "Customer Experience Analytics",
          "icon": "/app/images/wxm.bcd45cc3.svg",
          "iconType": "other",
          "navigateTo": "wxm-metrics",
          "align": "top"
        },
        "page": {
          "id": "wxm-metrics",
          "widgets": {
            "comp1": {
              "comp": "agentx-wc-cloudcherry-widget",
              "attributes": {
                "metrics": true
              },
              "properties": {
                "userModel": "$STORE.app.userModel",
                "spaceId": "",
                "metricsId": "",
                "teamId": "$STORE.agent.teamName",
                "isDarkMode": "$STORE.app.darkMode"
              }
            }
          },
          "layout": {
            "areas": [["comp1"]],
            "size": {
              "cols": [1],
              "rows": [1]
            }
          }
        },
        "visibility": "WXM_METRICS"
      },
      {
        "nav": {
          "label": "Sample Address Book",
          "icon": "assign-privilege",
          "iconType": "momentum",
          "navigateTo": "address-book",
          "align": "top"
        },
        "page": {
          "id": "sample-address-book",
          "widgets": {
            "main": {
              "comp": "div",
              "style": {
                "height": "100%",
                "overflow": "scroll"
              },
              "children": [
                {
                  "comp": "sa-address-book",
                  "script": "http://localhost:5000/build/bundle.js",
                  "attributes": {
                    "darkmode": "$STORE.app.darkMode"
                  },
                  "properties": {
                    "darkmode": "$STORE.app.darkMode",
                    "myOutdialEPID":"YOUR OUTDIAL EP ID FROM WXCC ADMIN PORTAL",
                    "realName1":"Dufus Name1",
                    "realEmail1":"dufus1@cisco.com",
                    "realNumber1":"(303) 123-4567",
                    "realName2":"Dufas Name2",
                    "realEmail2":"dufas2@cisco.com",
                    "realNumber2":"(714) 123-4567",
                    "realName3":"Dufas Name3",
                    "realEmail3":"dufas3@cisco.com",
                    "realNumber3":"(720) 123-4567"
                  }
                }
              ]
            }
          },
          "layout": {
            "areas": [["main"]],
            "size": {
              "cols": [1],
              "rows": [1]
            }
          }
        }
      }
    ]
  }
}
